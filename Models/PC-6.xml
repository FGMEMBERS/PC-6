<!-- propeller -->
 <animation>
  <type>select</type>  <object-name>prop.1</object-name>
  <object-name>prop.2</object-name>
  <object-name>prop.3</object-name>
  <condition>
   <less-than>
    <property>engines/engine[0]/rpm</property>
    <value>1000</value>
   </less-than>
  </condition>
 </animation>
 <animation>
  <type>spin</type>
  <object-name>prop.1</object-name>
  <object-name>prop.2</object-name>
  <object-name>prop.3</object-name>
  <property>engines/engine[0]/rpm</property>
  <factor>-0.479</factor>
  <center>
   <x-m>0.318</x-m>
   <y-m>0.0</y-m>
   <z-m>0.004</z-m>
  </center>
  <axis>
   <x1-m>0.424</x1-m>
   <y1-m>0.0</y1-m>
   <z1-m>0.009</z1-m>
   <x2-m>-0.021</x2-m>
   <y2-m>0.0</y2-m>
   <z2-m>-0.013</z2-m>
  </axis>
 </animation>
<!-- Rudder -->
 <animation>
  <type>rotate</type>
  <object-name>rudder</object-name>
  <property>surface-positions/rudder-pos-norm</property>
  <factor>30.0</factor>

  <center>
   <x-m>9.738</x-m>
   <y-m>0.0</y-m>
   <z-m>1.868</z-m>
  </center>

  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>
<!-- Elevator -->
 <animation>
  <type>rotate</type>
  <object-name>elevator.L</object-name>
  <object-name>elevator.R</object-name>
  <property>surface-positions/elevator-pos-norm</property>
  <interpolation>
   <entry>
    <ind>-1.0</ind>
    <dep>-25.0</dep>
   </entry>
   <entry>
    <ind>0.0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.00</ind>
    <dep>15.0</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>10.328</x-m>
   <y-m>0.0</y-m>
   <z-m>0.498</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
   </axis>
 </animation>
<!-- doors movement -->
<animation>
  <type>rotate</type>
  <object-name>pdoor.L</object-name>
  <object-name>phandle.L</object-name>
  <property>controls/doors/door0/position-norm</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0.0</dep>
   </entry>
  <entry>
    <ind>1.00</ind>
    <dep>-100.0</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>3.256</x-m>
   <y-m>-0.602</y-m>
   <z-m>0.106</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>
<animation>
  <type>rotate</type>
  <object-name>pdoor.R</object-name>
  <object-name>phandle.R</object-name>
  <property>controls/doors/door1/position-norm</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0.0</dep>
   </entry>
  <entry>
    <ind>1.00</ind>
    <dep>100.0</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>3.256</x-m>
   <y-m>0.602</y-m>
   <z-m>0.106</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>
<animation>
  <type>rotate</type>
  <object-name>swingdoor.F</object-name>
  <object-name>fswingdoorhandle</object-name>
  <property>controls/doors/door2/position-norm</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0.0</dep>
   </entry>
  <entry>
    <ind>1.00</ind>
    <dep>-100.0</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>3.991</x-m>
   <y-m>-0.601</y-m>
   <z-m>0.143</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>
<animation>
  <type>rotate</type>
  <object-name>swingdoor.R</object-name>
  <object-name>rswingdoorhandle</object-name>
  <property>controls/doors/door3/position-norm</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0.0</dep>
   </entry>
  <entry>
    <ind>1.00</ind>
    <dep>100.0</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>5.547</x-m>
   <y-m>-0.601</y-m>
   <z-m>0.025</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>
 <animation>
  <type>translate</type>
  <object-name>slidedoor</object-name>
  <object-name>sldoorhandle</object-name>
  <property>controls/doors/door4/position-norm</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0.</dep>
   </entry>
   <entry>
    <ind>1.00</ind>
    <dep>1.5</dep>
   </entry>
  </interpolation>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>
<!-- doors select -->
 <animation>
  <type>select</type>  <object-name>pdoor.L</object-name>
  <object-name>phandle.L</object-name>
  <condition>
   <equals>
    <property>controls/doors/door0/enabled</property>
    <value>1</value>
   </equals>
  </condition>
 </animation>
 <animation>
  <type>select</type>  <object-name>pdoor.R</object-name>
  <object-name>phandle.R</object-name>
  <condition>
   <equals>
    <property>controls/doors/door1/enabled</property>
    <value>1</value>
   </equals>
  </condition>
 </animation>
 <animation>
  <type>select</type>  <object-name>swingdoor.R</object-name>
  <object-name>swingdoor.F</object-name>
  <object-name>rswdoorhandle</object-name>
  <object-name>fswdoorhandle</object-name>
  <condition>
   <equals>
    <property>controls/doors/door2/enabled</property>
    <value>1</value>
   </equals>
  </condition>
 </animation>
 <animation>
  <type>select</type>  <object-name>slidedoor</object-name>
  <object-name>sldoorhandle</object-name>
  <condition>
   <equals>
    <property>controls/doors/door4/enabled</property>
    <value>1</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>phandle.L</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>pc6.toggle_pilotdoor()</script>
    </binding>
  </action>
</animation>
 <animation>
  <type>pick</type>
  <object-name>phandle.R</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>pc6.toggle_copilotdoor()</script>
    </binding>
  </action>
</animation>
 <animation>
  <type>pick</type>
  <object-name>sldoorhandle</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>pc6.toggle_slidedoor()</script>
    </binding>
  </action>
</animation>
  <model> 
   <path>Aircraft/PC-6/Models/pdisk.xml</path>
   <name>pdisk</name>
   <nopreview/>
    <offsets> 
     <x-m>0.35</x-m>
     <y-m>0.0</y-m>
     <z-m>0.0</z-m>
		 <pitch-deg>-2.0</pitch-deg>
    </offsets>
  </model>
    <model> 
      <path>Aircraft/PC-6/Models/pilot.xml</path>
	  <name>pilot</name>
   <nopreview/>
      <offsets> 
        <x-m>3.65</x-m>
        <y-m>-0.3</y-m>
        <z-m>0.0</z-m>
      </offsets>
    </model>
 <animation>
  <type>select</type>
  <object-name>pilot</object-name>
  <condition>
   <greater-than>
    <property>sim/current-view/view-number</property>
    <value>0.0</value>
   </greater-than>
  </condition>
 </animation>

    <model> 
      <path>Aircraft/PC-6/Models/transparent.xml</path>
	  <name>transparent</name>
      <offsets> 
        <x-m>0.0</x-m>
        <y-m>0.0</y-m>
        <z-m>0.0</z-m>
      </offsets>
    </model>


</PropertyList>
